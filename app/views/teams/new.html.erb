<div class="team-box col-10 offset-1">
  <h1>Enter your team details</h1>
  <%= simple_form_for @team do |f| %>
    <%= f.input :name %>
    <%= f.input :location %>
    <%= f.input :stadium %>
    <%= f.input :description %>
    <br />
    <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>